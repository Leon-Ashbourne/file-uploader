<html>
    <head>
        <title>Folders</title>
    </head>
    <body>
        <%- include("../../navbar")%>
        <main>
            <div>
                <p><a href="/library"> Back to Library</a></p>
            </div>
                          <div>
                    <form action="#" method="post" enctype="multipart/form-data">
                        <section>
                            <label for="files">choose files</label>
                            <input
                                type="file"
                                name="files"
                                id="files"
                                accept=".jpg, .jpeg, .png, .svg, .pdf, .webp, .doc, .docx "
                                multiple />
                        </section>
                        <section>
                            <button id="submit">Submit</button>
                        </section>
                    </form>
                </div>
            <div>
                <%- include("../files") %>
            </div>
        </main>
    </body>
</html>